    "handlebars": "^4.7.7",
    "justified-layout": "^4.1.0",
    "lodash-es": "^4.17.21",
    "luxon": "^3.2.1",
    "maplibre-gl": "^3.6.0",
    "socket.io-client": "^4.6.1",
    "svelte-local-storage-store": "^0.6.0",
    "svelte-maplibre": "^0.7.0",
    "thumbhash": "^0.1.1"
