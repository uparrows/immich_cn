    "@types/dom-to-image": "^2.6.4",
    "@types/justified-layout": "^4.1.0",
    "@types/lodash-es": "^4.17.6",
    "@types/luxon": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.4",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
