    "eslint-plugin-svelte": "^2.30.0",
    "factory.ts": "^1.3.0",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.4.21",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
